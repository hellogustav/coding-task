{
  "name": "gustav-enroll-frontend",
  "version": "1.0.0",
  "description": "The front-end for the Gustav Enroll project",
  "repository": {
    "type": "git",
    "url": "git://github.com/hellogustav/gustav-enroll-frontend.git"
  },
  "engines": {
    "yarn": "^1.22.0",
    "npm": ">=6",
    "node": "^16.10.0"
  },
  "author": "",
  "contributors": [],
  "license": "UNLICENSED",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets env,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js",
    "build:deployment": "cross-env NODE_ENV=production MODE=deployment webpack --config internals/webpack/webpack.prod.babel.js",
    "build:backend-dev": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.backend-dev.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "npm run test:clean && rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development node -r dotenv/config server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node -r dotenv/config server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "start:test": "cross-env NODE_ENV=test node -r dotenv/config server dotenv_config_path=.env.test",
    "start:local": "REST_API_URL=http://localhost:4000/api SOCKET_URL=ws://localhost:4000/socket yarn start",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "prettier_check": "prettier --check '**/*.js'",
    "format": "prettier --write .",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "precommit": "lint-staged",
    "postinstall": "patch-package",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "storybook": "NODE_ENV=storybook ./node_modules/.bin/start-storybook -p 6006",
    "build:storybook": "SOCKET_URL=wss://storybook.gustav.build/socket ./node_modules/.bin/build-storybook -o build-storybook",
    "deploy:preview:app": "npm run build && node_modules/netlify-cli/bin/cli.js deploy --site-id de6a6d6a-0ba3-4854-b9d1-145a7203e059 --path build",
    "deploy:preview:storybook": "npm run build:storybook && node_modules/netlify-cli/bin/cli.js deploy --site-id 9356a648-2d9b-41de-8b16-6eefd296acb6 --path build-storybook",
    "svgr": "svgr"
  },
  "yarn": {
    "cacheFolder": "npm-packages-offline-cache"
  },
  "lint-staged": {
    "*.js": [
      "lint:eslint",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "jest": {
    "globals": {
      "REST_API_URL": "https://app.gustav.build/api",
      "COMMIT_REF": "COMMIT_REF"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/*/RbGenerated*/*.{js,jsx}",
      "!app/app.js",
      "!app/routes.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 6,
        "branches": 3,
        "functions": 3,
        "lines": 3
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/internals/mocks/image.js",
      ".*\\.(html)$": "<rootDir>/internals/mocks/html.js"
    },
    "setupFiles": [
      "<rootDir>/internals/testing/test-bundler.js",
      "<rootDir>/internals/mocks/localStorage.js"
    ],
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "pre-commit": [
    "precommit",
    "e2e"
  ],
