{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:security/recommended", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": ["redux-saga", "react", "jsx-a11y", "security"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "arrow-parens": ["error", "always"],
    "arrow-body-style": [2, "as-needed"],
    "comma-dangle": [2, "always-multiline"],
    "curly": 2,
    "import/imports-first": 0,
    "import/newline-after-import": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 0,
    "import/no-unresolved": 2,
    "import/prefer-default-export": 0,
    "jsx-a11y/aria-props": 2,
    "jsx-a11y/heading-has-content": 0,
    "jsx-a11y/mouse-events-have-key-events": 2,
    "jsx-a11y/role-has-required-aria-props": 2,
    "jsx-a11y/role-supports-aria-props": 2,
    "max-len": 0,
    "newline-per-chained-call": 0,
    "no-confusing-arrow": 0,
    "no-console": 1,
    "no-use-before-define": 0,
    "prefer-template": 2,
    "class-methods-use-this": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-first-prop-new-line": [2, "multiline"],
    "react/jsx-filename-extension": 0,
    "react/jsx-no-target-blank": 2,
    "react/require-extension": 0,
    "react/self-closing-comp": 0,
    "redux-saga/no-yield-in-race": 2,
    "redux-saga/yield-effects": 2,
    "require-yield": 0,
    "import/no-webpack-loader-syntax": 0,
    "security/detect-object-injection": 0,
    "security/detect-child-process": 0,
    "security/detect-non-literal-fs-filename": 0,
    "security/detect-non-literal-regexp": 0,
    "security/detect-unsafe-regex": 2,
    "security/detect-buffer-noassert": 2,
    "security/detect-disable-mustache-escape": 2,
    "security/detect-eval-with-expression": 2,
    "security/detect-no-csrf-before-method-override": 2,
    "security/detect-non-literal-require": 2,
    "security/detect-possible-timing-attacks": 1,
    "security/detect-pseudoRandomBytes": 2,
    "react/jsx-props-no-spreading": 0,
    "react/require-default-props": 0,
    "prefer-promise-reject-errors": 0,
    "jsx-a11y/no-autofocus": 0,
    "react/static-property-placement": [2, "property assignment"],
    "camelcase": [
      "error",
      {
        "allow": [
          "UNSAFE_componentWillMount",
          "git_commit",
          "UNSAFE_componentWillReceiveProps"
        ],
        "properties": "never",
        "ignoreDestructuring": true,
        "ignoreImports": true
      }
    ],
    "react/no-array-index-key": 2
  },
  "settings": {
    "import/resolver": {
      "babel-module": {},
      "webpack": {
        "config": "./internals/webpack/webpack.prod.babel.js"
      }
    }
  }
}
